{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/ws",
      "allowedRoles": ["anonymous"],
      "methods": ["GET", "POST", "OPTIONS", "HEAD"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "*",
        "Upgrade": "*",
        "Connection": "Upgrade"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "networking": {
    "allowedIpRanges": ["any"]
  }
}