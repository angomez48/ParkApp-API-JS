<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <webSocket enabled="true" />
    <handlers>
      <add name="iisnode" path="websocket-server.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="WebSocket" patternSyntax="ECMAScript">
          <match url="/ws" />
          <action type="Rewrite" url="websocket-server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>